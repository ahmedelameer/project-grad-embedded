#include"uart.h"


int main(void){
	DDRD|=0x80;
	PORTD=(1<<7);
	unit8 data[14] ={ 0xAA,0x44,0x00,0x02,0x00,0x00,0x00,0x02,0x90,0xFA,0xCC,0x33,0xC3,0x3C},i;
	UART_init();




	while(1){
		for(i=0;i<14;i++){

 			UART_sendByte(data[i]);
			//_delay_ms(10);

		}

	}
	return 0;
}
